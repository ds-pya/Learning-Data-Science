override fun onHandleAssist(
    data: Bundle?,
    structure: AssistStructure?,
    content: AssistContent?
) {
    super.onHandleAssist(data, structure, content)

    Log.d("CalbakAssist", "====== onHandleAssist() ======")

    // 1) AssistContent 기반 정보
    val intent = content?.intent
    val webUri = content?.webUri
    val clip = content?.clipData
    val extras = content?.extras

    Log.d("CalbakAssist", "intent          = $intent")
    Log.d("CalbakAssist", "intent.data     = ${intent?.data}")
    Log.d("CalbakAssist", "webUri          = $webUri")
    Log.d("CalbakAssist", "clipData        = $clip")
    Log.d("CalbakAssist", "extras keys     = ${extras?.keySet()?.joinToString()}")

    // 2) AssistStructure 쪽 정보 (어떤 액티비티/앱인지)
    if (structure != null) {
        Log.d("CalbakAssist", "activityComponent = ${structure.activityComponent}")
        Log.d("CalbakAssist", "windowNodeCount   = ${structure.windowNodeCount}")
    } else {
        Log.d("CalbakAssist", "structure is null")
    }

    // data Bundle 안에 뭐가 실려 오는지
    if (data != null) {
        Log.d("CalbakAssist", "data keys        = ${data.keySet().joinToString()}")
    } else {
        Log.d("CalbakAssist", "data is null")
    }
}